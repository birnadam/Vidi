    "jwt-simple": "^0.5.6",
    "mongoose": "^5.6.6",
    "morgan": "^1.9.1",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "socket.io": "^2.2.0",
    "validator": "^11.1.0"
  },
